{
    "name": "kseven/telegram-bot",
    "type": "library",
    "description": "Bot Para Telegram Em PHP",
    "keywords": ["telegram", "bot", "api"],
    "license": "MIT",
    "homepage": "https://github.com/k7brasil/TelegramBotCore",
    "support": {
        "issues": "https://github.com/k7brasil/TelegramBotCore/issues",
        "source": "https://github.com/k7brasil/TelegramBotCore"
    },
    "authors": [
        {
            "name": "K'Seven",
            "email": "contato@kseven.dev.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.1|^2.0|^3.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "dms/phpunit-arraysubset-asserts": "^0.2",
        "monolog/monolog": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "KSeven\\TelegramBot\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 3600
    }
}
